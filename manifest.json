{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KawalData Scrapper",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Pengambil Data Otomatis dari halaman web dan ekspor ke file Excel/CSV",
  "action": {
    "default_icon": "icon/kawaldata64.png"
  },
  "permissions": [
    "webRequest",
    "activeTab"
  ],
  "incognito": "split",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "icon/kawaldata16.png",
    "32": "icon/kawaldata32.png",
    "64": "icon/kawaldata64.png",
    "128":"icon/kawaldata128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "onload.css"
      ],
      "js": [
        "js/jquery-3.1.1.min.js",
        "js/sha256.min.js",
        "onload.js"
      ]
    }
  ]
}